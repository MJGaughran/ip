}
grecord(longout,"$(P)lo$(CHAN)") {
	field(DESC,"test%d")
	field(DTYP,"Hideos lo stringParm")
	field(OUT,"#C$(C) S0 @$(IPSLOT)-Serial[$(CHAN)]")
}
